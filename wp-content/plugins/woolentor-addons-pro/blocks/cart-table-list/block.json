            "type":"string",
            "default":"left"
        },
        "show_sku":{
            "type": "boolean",
            "default": true
        },
        "show_meta_data":{
            "type": "boolean",
            "default": true
        },
        "show_discount_percent":{
            "type": "boolean",
            "default": true
        },
