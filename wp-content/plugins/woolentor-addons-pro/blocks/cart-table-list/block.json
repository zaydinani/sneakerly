{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
    "name": "woolentor/cart-table-list",
    "category": "woolentor-cart",
    "title": "WL: Cart Table (List Style)",
    "description": "Display cart table list style.",
    "keywords": ["woolentor","shop table","cart table","table","woocommerce table","list style","table list","cart table list"],
    "supports": {
      "align": ["wide","full"]
    },
    "render": "file:./index.php",
    "attributes": {
        "blockUniqId":{
            "type": "string",
            "default": ""
        },
        "areaPadding": {
            "type": "object",
            "default": {
                "device": "desktop"
            },
            "style" : {
                "selector":"{{WOOLENTOR_WRAPPER}} { padding: {{areaPadding}} !important; }"
            }
        },
        "areaBGProperty":{
            "type": "object",
            "default": {
                "imageId": null,
                "imageUrl":"",
                "position": "",
                "attachment": "",
                "repeat": "",
                "size": ""
            },
            "style" : {
                "selector":"{{WOOLENTOR_WRAPPER}}"
            }
        },

        "layout":{
            "type":"string",
            "default":"1"
        },
        "verticalAlignment":{
            "type":"string",
            "default":"flex-start",
            "style" : {
                "selector":"{{WOOLENTOR_WRAPPER}} .woolentor-cart-product { align-items: {{verticalAlignment}}; }"
            }
        },
        "remove_product_link":{
            "type": "boolean",
